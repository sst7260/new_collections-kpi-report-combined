{
  "environments": [
    {
      "name": "dev_environment",
      "configOverride": {
        "defaultSchema": "new_rpt_Collections_KPI",
        "schemaSuffix": "DEV",
        "defaultDatabase": "springer-nature-analytics"
      },
      "schedules": [
        {
          "name": "dev_new_schedule",
          "cron": "15 16 * * mon,tue,wed,thu,fri,sat,sun",
          "tags": [
            "ct_1",
            "ct_2",
            "ct_3",
            "ct_4",
            "ct_5",
            "ct_8",
            "ct_9",
            "ct_9.5",
            "ct_10"
          ]
        },
        {
          "name": "dev_usage_schedule",
          "cron": "45 12 * * sun",
          "tags": [
            "ct_weekly_6",
            "ct_weekly_7"
          ]
        }
      ],
      "gitRef": "master"
    },
    {
      "name": "production",
      "configOverride": {
        "defaultSchema": "new_rpt_Collections_KPI_master",
        "defaultDatabase": "springer-nature-analytics"
      },
      "schedules": [
        {
          "name": "main_prod_daily",
          "cron": "54 6 * * mon,tue,wed,thu,fri,sat,sun",
          "tags": [
            "ct_1",
            "ct_2",
            "ct_3",
            "ct_4",
            "ct_5",
            "ct_8",
            "ct_9",
            "ct_9.5",
            "ct_10"
          ],
          "notify": [
            {
              "channel": "new_notification_channel",
              "statuses": [
                "SUCCESS",
                "FAILURE"
              ]
            }
          ]
        },
        {
          "name": "main_prod_weekly",
          "cron": "49 8 * * sun",
          "tags": [
            "ct_weekly_6",
            "ct_weekly_7"
          ]
        }
      ],
      "gitRef": "master"
    }
  ],
  "notificationChannels": [
    {
      "name": "new_notification_channel",
      "email": {
        "to": [
          "shreyas.shetty@springernature.com"
        ]
      }
    }
  ]
}